{
  "version": 2,
  "buildCommand": "echo 'No build needed'",
  "functions": {
    "dist/**/*.php": {
      "runtime": "@vercel/php@2.0.0"
    },
    "api/**/*.php": {
      "runtime": "@vercel/php@2.0.0"
    }
  },
  "rewrites": [
    {
      "source": "/webhook.php",
      "destination": "/dist/webhook.php"
    },
    {
      "source": "/api/game.php",
      "destination": "/dist/api/game.php"
    },
    {
      "source": "/setup/setup-webhook.php",
      "destination": "/api/setup-webhook.php"
    },
    {
      "source": "/api/setup-webhook.php",
      "destination": "/api/setup-webhook.php"
    },
    {
      "source": "/web/:path*",
      "destination": "/dist/web/:path*"
    }
  ]
}

