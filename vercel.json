{
  "version": 2,
  "builds": [
    {
      "src": "dist/web/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/webhook.php",
      "destination": "/api/webhook.js"
    },
    {
      "source": "/setup/setup-webhook.php",
      "destination": "/api/setup-webhook.js"
    },
    {
      "source": "/api/setup-webhook.php",
      "destination": "/api/setup-webhook.js"
    },
    {
      "source": "/api/check-webhook",
      "destination": "/api/check-webhook.js"
    },
    {
      "source": "/web/:path*",
      "destination": "/dist/web/:path*"
    }
  ],
  "headers": [
    {
      "source": "/web/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

