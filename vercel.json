{
  "version": 2,
  "rewrites": [
    {
      "source": "/webhook.php",
      "destination": "/api/webhook.js"
    },
    {
      "source": "/setup/setup-webhook.php",
      "destination": "/api/setup-webhook.js"
    },
    {
      "source": "/api/setup-webhook.php",
      "destination": "/api/setup-webhook.js"
    },
    {
      "source": "/api/check-webhook",
      "destination": "/api/check-webhook.js"
    },
    {
      "source": "/web/:path*",
      "destination": "/dist/web/:path*"
    }
  ],
  "routes": [
    {
      "src": "/web/assets/images/(.*)",
      "dest": "/dist/web/assets/images/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "headers": [
    {
      "source": "/web/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/public/web/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

